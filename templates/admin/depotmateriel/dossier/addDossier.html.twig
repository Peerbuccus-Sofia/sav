{% extends 'base.html.twig' %}

{% block title %}Dépôt de matériel{% endblock %}

{% block body %}
    <div class="row justify-content-center">
        <h1>Dépôt de matériel</h1>
    </div>
    {# fiche client #}
    {{ include ('admin/depotmateriel/client/ficheclientbydossier.html.twig', {client, materiel, pannes} )}}
    <br>
    {# fiche du materiel #}
    {{ include ('materiel/fichematerielbydossier.html.twig', {materiel, client, pannes} )}}
    <br>
    {# fiche de la panne #}
    {{ include ('admin/depotmateriel/panne/recappanne.html.twig', {materiel, client, pannes} )}}

    {# formulaire entité dossier #}
    {{ include ('admin/depotmateriel/dossier/_form.html.twig', {form : dossierform, button: 'Créer'} )}}

{% endblock %}
